{
  "name": "webpack-init",
  "version": "1.0.0",
  "description": "webpack automate workflow",
  "main": "./src/main.js",
  "scripts": {
    "build": "webpack",
    "build:dev": "node_modules/.bin/webpack --env=dev --progress --profile --colors",
    "build:prod": "node_modules/.bin/webpack --env=prod --progress --profile --colors",
    "watch": "webpack-dev-server --env=dev --open",
    "production": "node_modules/.bin/webpack -p"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ysneo/webpack-init.git"
  },
  "keywords": [],
  "author": "jih",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ysneo/webpack-init/issues"
  },
  "homepage": "https://github.com/ysneo/webpack-init#readme",
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-preset-env": "^1.6.0",
    "clean-webpack-plugin": "^0.1.16",
    "css-loader": "^0.28.4",
    "file-loader": "^0.11.2",
    "html-webpack-plugin": "^2.29.0",
    "style-loader": "^0.18.2",
    "underscore": "^1.8.3",
    "webpack": "^3.3.0",
    "webpack-dev-server": "^2.6.1"
  },
  "dependencies": {
    "lodash": "^4.17.4"
  }
}